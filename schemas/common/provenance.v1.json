  "$id": "gemantria://v1/common/provenance.v1.json",
  "title": "Provenance v1.0.0",
  "type": "object",
  "required": [
    "model",
    "seed",
    "ts_iso"
  ],
  "properties": {
    "model": {
      "type": "string",
